<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.userdata.presenters.ui.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
                android:id="@+id/action_mainFragment_to_elementFragment"
                app:destination="@id/elementFragment"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" app:popEnterAnim="@anim/nav_default_pop_enter_anim"/>
    </fragment>
    <fragment
        android:id="@+id/elementFragment"
        android:name="com.example.userdata.presenters.ui.main.ElementDetailFragment"
        android:label="fragment_element"
        tools:layout="@layout/fragment_element"/>
</navigation>